    "test": "turbo run test",
    "test:watch": "turbo run test:watch"
  },
  "private": true,
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "eslint-plugin-jest": "^27.2.3",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "turbo": "latest",
    "typescript": "^5.1.6"
  },
  "workspaces": [
    "applications/*",
    "libraries/*"
  ],
  "dependencies": {
    "@commercetools/platform-sdk": "^4.11.0",
    "@commercetools/sdk-client-v2": "^2.2.0",
    "@nestjs/config": "^3.0.0"
  }
